<?xml version="1.0" encoding="UTF-8"?>

<!-- 
tags meanings

<antara> - tag namespaces
<message name = "" mode= "" extension = "" nodeID = "" componentID = ""> - create message

<payload> - payload tag
<field name = "" type = "float/int/etc" value = "a/runtime" > - init byte field 

<enum num = ""> - define enumerates scope
<entry name = "" value = ""> define entry for enum

<description> - comments
-->


<antara>
    <message name = "heartbeat" mode = "RELIABLE_MODE,PUB_SUB" extension = "SIGNATURE_EXTENSION" nodeID = "10" componentID = "200">
        <enum num = "5">
            <entry name = "haha" value = "1">Hello Message</entry>

        </enum>
        <payload msgID = "1" length = "2">
            <field name = "a" type = "int32_t" ></field>
            <field name = "timestamp" type = "float"></field>

        </payload>
    </message>
</antara>